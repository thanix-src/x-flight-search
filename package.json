{
  "name": "xflightsearch-ui",
  "version": "1.0.0",
  "description": "UI Package for The X Flight Search System",
  "main": "app.js",
  "scripts": {
    "setup": "npm install && typings install",
    "clean-docs": "rimraf docs/**/*",
    "clean-build": "rimraf build/**/*",
    "clean": "npm run clean-docs && npm run clean-build",
    "build": "npm run clean && webpack --progress --colors",
    "serve": "webpack-dev-server --content-base app/ --progress --colors --inline --hot"
  },
  "keywords": [
    "flight", 
    "search",
    "ui"
  ],
  "author": "Thanikachalam Ananthakrishnan",
  "license": "UNLICENSED",
  "devDependencies": {
    "angular-mocks": "^1.5.3",
    "autoprefixer": "^6.3.3",
    "bootstrap-loader": "^1.0.1",
    "constants": "0.0.2",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^0.9.1",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.6.5",
    "ng-annotate-webpack-plugin": "^0.1.2",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "protractor": "^3.0.0",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.4.3",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.7.2",
    "tslint": "^3.2.1",
    "tslint-loader": "^2.1.0",
    "typings": "^0.7.12",
    "typescript": "^1.8.9",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-config": "^3.1.0",
    "webpack-dev-server": "^1.14.0",
    "webpack-stream": "^3.1.0"
  },
  "dependencies": {
    "@types/lodash": "^4.14.66",
    "angular": "^1.5.3",
    "bootstrap": "^3.3.6",
    "bootstrap-sass": "^3.3.6",
    "jquery": "^2.1.4",
    "lodash": "4.11.2",
    "socket.io-client": "^1.3.7"
  }
}
